import "math.lzl"

struct V2R {
    x: Real
    y: Real
}

struct V3R {
    x: Real
    y: Real
    z: Real
}

fn mag2(v: V2R): Real {
    return math::sqrt(v.x * v.x + v.y * v.y)
}

fn dot2(v1: V2R, v2: V2R): Real {
    return v1.x * v2.x + v1.y * v2.y
}

fn mag3(v: V3R): Real {
    return math::sqrt(v.x * v.x + v.y * v.y + v.z * v.z)
}

fn dot3(v1: V3R, v2: V3R): Real {
    return v1.x * v2.x + v1.y * v2.y + v1.z * v2.z
}