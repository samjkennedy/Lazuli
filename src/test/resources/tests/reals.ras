fn sqrt(S: Real): Real {
    iterations: Int = 100
    x0: Real = (S / 10.0)
    x1: Real = (x0 + (S / x0)) / 2.0
    for (i: Int = 1 to iterations) {
        tmp: Real = x0
        x0 = (x1 + (S / x1)) / 2.0
        x1 = tmp
    }
    return x0
}

print(sqrt(4.))
print(sqrt(5.0))
print(sqrt(35.2))
print(sqrt(125348.))