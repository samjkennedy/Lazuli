fn max(a: Int, b: Int): Int {
    if (a > b) {
        return a
    } else {
        if (a == b) {
            return a
        } else {
            return b
        }
    }
}

fn puti(i: Int) {
    print(i)
}

fn main() {
    if (true and false) {
        print("this is unreachable")
    } else {
        if (true or false) {
            if (true xor true) {
                print("this is also unreachable")
            } else {
                print("Hello there")
            }
        } else {
            print("this is unreachable too")
        }
    }

    n: Int = 1
    while (n < 10) {
        print(n)
        n = n + n
    }

    i: Int = 0
