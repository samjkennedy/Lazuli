        print(i)
        j: Int = 0
        while (j < 10) {
            print(j)
            j = j + 1
        }
        i = i + 1
    }

    r: Real = 0.
    while (r <= 10.) {
        print(r)
        r = r + 0.5
    }

    i: Int = 2
    puti(max(i, 3))
}