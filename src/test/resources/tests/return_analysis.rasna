fn badWhile(i: Int): Int {
    while (i < 0) {
        return 1
    }
}

fn badIf(i: Int): Int {
    if (i < 0) {
        return 1
    }
}

fn badIfElse(i: Int): Int {
    if (i < 0) {
        return 1
    } else if (i > 3) {
        return 2
    }
}

fn badFn(): Int {
    print("Hello")
}

fn badTuple(a: Int): Tuple {
    match (a) {
        1    -> return ("Hello", true)
        2    -> {
            print("2")
            return ("Goodbye", 6)
        }
        3 -> (1,)
        else -> return ("World", false, 1)
    }
}

fn unreachable(): Int {

    return 1
    print("Unreachable!")
}
