fn sort(array: Int[] ): Int[] {

   n: Int = len(array)
   for (i: Int = 1 to n) {
       for (j: Int = 0 to n - i) {
           if (array[j] > array[j + 1]) {
               tmp: Int = array[j]
               array[j] = array[j + 1]
               array[j + 1] = tmp
           }
       }
   }
   return array
}

fn main() {
    arr: Int[] = [6, 2, 8, 3, 4, 9, 5, 1, 7, 0]
    for (a: Int in sort(arr)) print(a)

    ints: Int[] = [1, 2, 3]
    for (i: Int in ints) print(i)

    bools: Bool[] = [true, false, true, true, false]
    for (b: Bool in bools) print(b)

    strings: String[] = ["Hello", "World"]
    for (s: String in strings) print(s)

    reals: Real[] = [1.414, 2.718, 3.141]
    for (r: Real in reals) print(r)
}
