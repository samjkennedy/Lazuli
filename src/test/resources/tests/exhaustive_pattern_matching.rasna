enum Color {
    Red
    Green
    Blue
    Yellow
    Orange
}

fn printCol(c: Color) {
    match (c) {
        Color.Red   => print("Red")
        Color.Green => print("Green")
        Color.Blue  => print("Blue")
    }
}

fn main() {

    i := 1

    match (i) {
        0 => print("Zero")
        1 => print("One")
    }
}