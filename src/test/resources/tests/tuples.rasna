fn multiReturn(a: Int): (String, Bool) {
    match (a) {
        1    => return ("Hello\n", true)
        else => return ("World\n", false)
    }
}

fn main() {
    t := ("Hello\n", 1, true)

    s: String = t.0
    print(s)

    print(t.0)
    print(t.1)
    print(t.2)

    sb := multiReturn(2)
    print(sb.0)
    print(sb.1)

    //Tuples of only one element require a trailing comma:
    singleton: (Int) = (10,)
    print(singleton.0)

    nested := (1, (2, (3, true)))
    print(nested.0)
    print((nested.1).0)
    print(((nested.1).1).0)
    print(((nested.1).1).1)
}