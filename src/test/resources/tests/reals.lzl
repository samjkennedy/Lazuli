Real sqrt(Real S) {
    Int iterations = 100
    Real x0 = S / 10.0
    Real x1 = (x0 + (S / x0)) / 2.0
    for (Int i = 1 to iterations) {
        Real tmp = x0
        x0 = (x1 + (S / x1)) / 2.0
        x1 = tmp
    }
    return x0
}

print(sqrt(4.))
print(sqrt(5.0))
print(sqrt(35.2))
print(sqrt(125348.))