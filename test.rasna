struct Vec {
    x: Int
    y: Int
}

fn scale(vec: Vec, scalar: Int): Vec {
    return Vec{vec.x * scalar, vec.y * scalar}
}

fn sum(vec: Vec): Int {
    return vec.x + vec.y
}

fn printX(v: Vec) {
    print(v.x)
}

fn main() {
    v: Vec = {2, 3}
    v.x = 5
    printX(v)
    sum: Int = v.scale(2).sum()

    print(sum)
}