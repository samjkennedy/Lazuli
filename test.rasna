fn sort(ref array: Int[]) {

   n: Int = len(array)
   for (i: Int = 1 to n) {
       for (j: Int = 0 to n - i) {
           if (array[j] > array[j + 1]) {
               tmp: Int = array[j]
               array[j] = array[j + 1]
               array[j + 1] = tmp
           }
       }
   }
}

fn main() {
    arr: Int[] = [80, 42, 34, 7, 95, 77, 85, 18, 2, 11, 31, 25, 61, 1, 75, 84, 82, 49, 9, 83, 72, 67, 59, 92, 21, 51, 90, 28, 16, 62, 48, 73, 50, 96, 93, 22, 89, 14, 76, 66, 44, 19, 26, 43, 81, 53, 24, 13, 65, 39, 99, 55, 78, 98, 60, 64, 15, 56, 71, 87, 58, 100, 63, 46, 35, 8, 23, 91, 10, 4, 47, 6, 29, 33, 12, 97, 54, 30, 32, 38, 45, 94, 20, 40, 17, 88, 69, 5, 86, 68, 3, 79, 27, 41, 52, 70, 36, 74, 37, 57]

    arr->sort()

    for (i: Int in arr) {
        print(i)
    }
}