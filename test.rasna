interface Shape {
    area(): Real
}

interface Named {
    name(): String
}

struct Rect {
    width: Real
    height: Real
}

struct Circle {
    radius: Real
}

fn area(rect: Rect): Real {
    return rect.width * rect.height
}

fn name(rect: Rect): String {
    return "Rect"
}

fn name(rect: Circle): String {
    return "Circle"
}

fn area(circle: Circle): Real {
    return 3.14159 * circle.radius * circle.radius
}

fn measure(s: Shape) {
    print("The shape's area is: ")
    print(s.area())
}

fn main() {
    r := Rect{2.0, 3.0}
    c := Circle{2.0}

    r.measure()
    c.measure()
}