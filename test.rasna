interface Shape {
    area(): Real
}

struct Rect { // <: Shape {
    width: Real
    height: Real
}

struct Circle { // <: Shape {
    radius: Real
}

fn area(rect: Rect): Real {
    return rect.width * rect.height
}

fn area(circle: Circle): Real {
    return 3.14159 * circle.radius * circle.radius
}

