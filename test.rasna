enum Color {
    Red
    Blue
    Green
}

fn printColor(ref const c: Color) {
    if (c == Color.Red) {
        print("Red")
    }
    if (c == Color.Blue) {
        print("Blue")
    }
    if (c == Color.Green) {
        print("Green")
    }
}

fn sort(ref array: Color[]) {

   n: Int = len(array)
   for (i: Int = 1 to n) {
       for (j: Int = 0 to n - i) {
           if (array[j] > array[j + 1]) {
               tmp: Color = array[j]
               array[j] = array[j + 1]
               array[j + 1] = tmp
           }
       }
   }
}

fn flip(cols: (Color, Color)): (Color, Color) {
    return (cols.1, cols.0)
}

fn main() {

    t: (Color, Color) = (Color.Red, Color.Blue)
    t.0->printColor()
    t.1->printColor()

    t2: (Color, Color) = t.flip()
    t2.0->printColor()
    t2.1->printColor()

 }