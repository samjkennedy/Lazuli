fn toArray(val: Real, length: Int): Real[] {
    array: Real[] = Real[length]
    for (i: Int = 0 to length) {
        array[i] = val
    }
    return array
}

fn elementArrayOf(length: Int, val: Real): Real[] {
    array: Real[] = Real[length]
    for (i: Int = 0 to length) {
        array[i] = val
    }
    return array
}

fn main() {
    arr: Real[] = 3.elementArrayOf(1.) //returns [1.0, 1.0, 1.0]
    print(len(arr)) //prints 3
}