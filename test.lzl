
IntArray sort(IntArray array) {

   Int n = len(array)
   for (Int i = 1 to n) {
       for (Int j = 0 to n - i) {
           if (array[j] > array[j + 1]) {
               Int tmp = array[j]
               array[j] = array[j + 1]
               array[j + 1] = tmp
           }
       }
   }
   return array
}

IntArray arr = [6, 2, 8, 3, 4, 9, 5, 1, 7, 0]
for (Int a in sort(arr)) print(a)